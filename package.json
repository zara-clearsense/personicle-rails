{
  "name": "personicle",
  "private": true,
  "scripts": {
    "yarn test": "jest",
    "yarn install": "yarn install --check-files",
    "rails assets:precompile": "yarn install --check-files; rails assets:precompile",
    "rails server": "rails server",
    "webpack-dev-server": "./bin/webpack-dev-server"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/app/javascript/parts/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(vueonrails)/)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/config/webpack/"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  },
  "dependencies": {
    "@babel/preset-react": "^7.16.7",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@hotwired/stimulus": "^3.0.1",
    "@hotwired/turbo-rails": "^7.1.3",
    "@material-ui/icons": "^4.11.3",
    "@mui/icons-material": "^5.8.3",
    "@mui/material": "^5.8.3",
    "@popperjs/core": "^2.11.4",
    "@rails/actioncable": "^6.0.0",
    "@rails/activestorage": "^6.0.0",
    "@rails/ujs": "^6.0.0",
    "@rails/webpacker": "5.4.3",
    "avsc": "^5.7.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "bootstrap": "^4.5.2",
    "chart.js": "^3.9.1",
    "chartkick": "^4.2.0",
    "jquery": "^3.5.1",
    "material-ui-image": "^3.3.2",
    "popper.js": "^1.16.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react_ujs": "^2.6.1",
    "react-bootstrap": "^2.2.1",
    "react-chartjs-2": "^4.3.1",
    "react-chrono": "^1.18.0",
    "react-dom": "^17.0.2",
    "react-google-charts": "^4.0.0",
    "react-router-dom": "^6.3.0",
    "react-simple-image-slider": "^2.4.1",
    "turbolinks": "^5.2.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12"
  },
  "version": "0.1.0",
  "devDependencies": {
    "babel-jest": "^28.1.0",
    "jest": "^28.1.0",
    "webpack-dev-server": "^3"
  }
}
