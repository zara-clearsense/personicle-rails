<div class="card">
  <div class="card-header bg-transparent">
    <div class="row align-items-center">
      <div class="col">
        <h6 class="text-black text-uppercase ls-1 mb-1">Overview</h6>
        <h5 class="h3 text-black mb-0"><span class="hovertext" data-hover="A timeline chart which logs every activity a user does including sleeping, running, biking, eating food, or any other activity. The user can toggle the slider to adjust the time frame and view activities between only certain start and end dates."><i class="fa fa-info-circle" aria-hidden="true"></i></span> Your Activites</h5>
      </div>
      <div class="col">
        <ul class="nav nav-pills justify-content-end">
          <li class="nav-item mr-2 mr-md-0" data-toggle="chart" data-target="#chart-sales-dark" data-update='{"data":{"datasets":[{"data":[0, 20, 10, 30, 15, 40, 20, 60, 60]}]}}' data-prefix="$" data-suffix="k">
             <%# <a href="#" class="nav-link py-2 px-3 active" data-toggle="tab">
              <span class="d-none d-md-block">Month</span>
              <span class="d-md-none">M</span>
            </a> %>
          </li>
          <li class="nav-item" data-toggle="chart" data-target="#chart-sales-dark" data-update='{"data":{"datasets":[{"data":[0, 20, 5, 25, 10, 30, 15, 40, 40]}]}}' data-prefix="$" data-suffix="k">
            <%# <a href="#" class="nav-link py-2 px-3" data-toggle="tab">
              <span class="d-none d-md-block">Week</span>
              <span class="d-md-none">W</span>
            </a>  %>
              <%= form_with url: pages_dashboard_path, local: true, method: :post do  %>
                <input type="hidden" name="refresh" value="hard_refresh">
                <button  class="btn btn-primary" type="submit">Refresh</button>
              <% end %>
          </li>
        </ul>
      </div>
    </div>
  </div>
  <div class="card-body">
    <!-- Chart -->
    <div class="chart">
      <!-- Chart wrapper -->
      <%# <% puts user_data %> 
      <%# <%= timeline [
        # obj
        ["Washington", "1789-04-29", "1797-03-03"],
        ["Adams", "1797-03-03", "1801-03-03"],
        ["Jefferson", "1801-03-03", "1809-03-03"]
      ] %> 
      
      <% if user_data.length() != 0 %> 
         <%= react_component("TimeLine", { user_data: user_data}) %>
      <%else%>
        <h3 style="color:black;">No activities for the past 3 months</h3>
      <%end%>
     
      <%# <canvas id="chart-sales-dark" class="chart-canvas"></canvas> %>
    </div>
  </div>
</div>
